<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="bitches_1" author="solncev">
        <loadUpdateData tableName="users" primaryKey="id" file="db/bitches/users.csv">
            <column name="id" type="numeric"/>
        </loadUpdateData>
    </changeSet>

    <changeSet id="bitches_2" author="solncev">
        <loadUpdateData tableName="posts" primaryKey="id" file="db/bitches/posts.csv">
            <column name="id" type="numeric"/>
        </loadUpdateData>
    </changeSet>

    <changeSet id="bitches_3" author="solncev">
        <loadUpdateData tableName="messages" primaryKey="id" file="db/bitches/messages.csv">
            <column name="id" type="numeric"/>
        </loadUpdateData>
    </changeSet>
</databaseChangeLog>